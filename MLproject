name: Airline_Sentiment_Analysis

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      epochs: {type: int, default: 3}
      lr: {type: float, default: 5e-5}
      batch_size: {type: int, default: 16}
    command: "python main.py --epochs {epochs} --lr {lr} --batch_size {batch_size}"

